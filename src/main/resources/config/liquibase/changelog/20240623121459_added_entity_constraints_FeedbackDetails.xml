<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity FeedbackDetails.
    -->
    <changeSet id="20240623121459-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="feedbackdetails_id"
                                 baseTableName="feedback_details"
                                 constraintName="fk_feedback_details__feedbackdetails_id"
                                 referencedColumnNames="id"
                                 referencedTableName="feedback"
                                 />

        <addForeignKeyConstraint baseColumnNames="feedbacksubtypes_id"
                                 baseTableName="feedback_details"
                                 constraintName="fk_feedback_details__feedbacksubtypes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="feedback_sub_type"
                                 />

        <addForeignKeyConstraint baseColumnNames="ratings_id"
                                 baseTableName="feedback_details"
                                 constraintName="fk_feedback_details__ratings_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rating"
                                 />
    </changeSet>
</databaseChangeLog>
