    <!--
        Added the constraints for entity Feedback.
    -->
    <changeSet id="20240623121458-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="requesters_id"
                                 baseTableName="feedback"
                                 constraintName="fk_feedback__requesters_id"
